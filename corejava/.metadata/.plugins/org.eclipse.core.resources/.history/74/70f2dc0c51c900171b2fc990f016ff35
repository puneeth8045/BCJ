package com.crmindz.assignments.Operators;

public class NewThread {
	
	public static void main(String[] args) {
	 int[] a = {11,2,31,4,51};	
		int smallest = a[0];
	for(int i=0;i<a.length;i++) {
		if(a[i]<=smallest) {
			smallest = a[i];
		}
	}
	System.out.println(smallest);
	}
	}
		
		
		
	
	





	
	
