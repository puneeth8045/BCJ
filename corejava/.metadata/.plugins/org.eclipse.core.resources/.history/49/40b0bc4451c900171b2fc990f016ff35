package com.crmindz.assignments.Operators;

public class NewThread {
	
	public static void main(String[] args) {
	String s = "vowel";
	char[] c = s.toCharArray();
	int vowel = 0;
	for(int i=0;i<c.length;i++) {
		switch (c[i]) { case 'a':
			case 'e': 
				case 'i': 
					case 'o': 
						case 'u':
							vowel++;
		}
			
		System.out.println(vowel);

			
	}System.out.println(vowel);
	}
	}
		
		
		
	
	





	
	
