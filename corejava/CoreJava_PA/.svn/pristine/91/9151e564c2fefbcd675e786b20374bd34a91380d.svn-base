package com.crmindz.assignments.Oops.Classes.medium.lab3;

/**
 * @author Puneeth
 *Entity class to populate date 
 */
public class MyDate {
      int month;
      int day;
      int year;
	public int getMonth() {
		return month;
	}
	public void setMonth(int month) {
		this.month = month;
	}
	public int getDay() {
		return day;
	}
	public void setDay(int day) {
		this.day = day;
	}
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
	public MyDate(int month, int day, int year) {
		super();
		this.month = month;
		this.day = day;
		this.year = year;
	}
       /**
     * Method to print date
     */
    public void displayDate(){
    	   System.out.println(month + "/" + day + "/" + year);
       }
}
