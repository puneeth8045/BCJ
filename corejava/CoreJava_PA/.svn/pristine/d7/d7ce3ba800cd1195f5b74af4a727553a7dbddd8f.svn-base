package com.crmindz.assignments.Oops.Classes.medium.lab6;

/**
 * @author Puneeth Class to show a method swap to swap the values of two
 *         objects.
 */
public class IntegerWrapper {
	private int a;

	public IntegerWrapper(int a) {
		this.a = a;
	}

	/**
	 * @param iw
	 * @param iw1
	 *            This method swaps the integer.
	 */
	public static void swap(IntegerWrapper iw, IntegerWrapper iw1) {
		int temp = iw.a;
		iw.a = iw1.a;
		iw1.a = temp;

	}

	public static void main(String[] args) {
		IntegerWrapper iw = new IntegerWrapper(1);
		IntegerWrapper iw1 = new IntegerWrapper(2);

		swap(iw, iw1);
		System.out.println("iw.a = " + iw.a);
		System.out.println("iw1.a = " + iw1.a);

	}

}
